							"script": {
								"exec": [
									"var body = JSON.parse(responseBody);",
									"pm.collectionVariables.set(\"bundleAppUrl\", body[\"uploadParameters\"][\"endpointURL\"]);",
									"pm.collectionVariables.set(\"bundleAppVersion\", body[\"version\"]);",
									"pm.collectionVariables.set(\"bundleAppFormDataKey\", body[\"uploadParameters\"][\"formData\"][\"key\"]);",
									"pm.collectionVariables.set(\"bundleAppFormDataPolicy\", body[\"uploadParameters\"][\"formData\"][\"policy\"]);",
									"pm.collectionVariables.set(\"bundleAppFormDataSignature\", body[\"uploadParameters\"][\"formData\"][\"x-amz-signature\"]);",
									"pm.collectionVariables.set(\"bundleAppFormDataCredential\", body[\"uploadParameters\"][\"formData\"][\"x-amz-credential\"]);",
									"pm.collectionVariables.set(\"bundleAppFormDataDate\", body[\"uploadParameters\"][\"formData\"][\"x-amz-date\"]);",
									"pm.collectionVariables.set(\"bundleAppFormDataToken\", body[\"uploadParameters\"][\"formData\"][\"x-amz-security-token\"]);",
									"pm.collectionVariables.set(\"bundleAppFormRedirect\", body[\"uploadParameters\"][\"formData\"][\"success_action_redirect\"]);",

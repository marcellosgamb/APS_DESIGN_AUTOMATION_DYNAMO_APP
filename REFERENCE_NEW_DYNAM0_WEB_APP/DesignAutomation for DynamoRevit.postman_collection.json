				{
					"name": "Create a New Activity",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var body = JSON.parse(responseBody);",
									"pm.collectionVariables.set(\"activityVersion\", body[\"version\"]);",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"var activitId = pm.environment.get(\"activityId\");\r",
									"console.log(activitId);\r",
									"\r",
									"pm.environment.set(\"activity\", JSON.stringify({\r",
									"            \"id\": activitId,\r",
									"            \"commandLine\": [ `$(engine.path)\\\\\\\\revitcoreconsole.exe /i \\\"$(args[rvtFile].path)\\\" /al \\\"$(appbundles[${pm.environment.get(\"appBundleName\")}].path)\\\"` ],\r",
									"            \"parameters\": {\r",
									"             \"rvtFile\": {\r",
									"                \"zip\": false,\r",
									"                \"ondemand\": false,\r",
									"                \"verb\": \"get\",\r",
									"                \"description\": \"Input Revit model\",\r",
									"                \"required\": true,\r",
									"                \"localName\": \"$(rvtFile)\"\r",
									"              },\r",
									"             \"runRequest\": {\r",
									"                \"zip\": false,\r",
									"                \"ondemand\": false,\r",
									"                \"verb\": \"get\",\r",
									"                \"description\": \"Input Revit model\",\r",
									"                \"required\": false,\r",
									"                \"localName\": \"run.json\"\r",
									"              },\r",
									"              \"pythonLibs\": {\r",
									"                \"zip\": true,\r",
									"                \"ondemand\": false,\r",
									"                \"verb\": \"get\",\r",
									"                \"description\": \"Python libs\",\r",
									"                \"required\": false,\r",
									"                \"localName\": \"pythonDependencies\"\r",
									"              },\r",
									"              \"packages\": {\r",
									"                \"zip\": true,\r",
									"                \"ondemand\": false,\r",
									"                \"verb\": \"get\",\r",
									"                \"description\": \"Dynamo packages\",\r",
									"                \"required\": false,\r",
									"                \"localName\": \"packages\"\r",
									"              },\r",
									"              \"dynResult\": {\r",
									"                \"zip\": false,\r",
									"                \"ondemand\": false,\r",
									"                \"verb\": \"put\",\r",
									"                \"description\": \"Results\",\r",
									"                \"required\": false,\r",
									"                \"localName\": \"result.json\"\r",
									"              },\r",
									"              \"rvtResult\": {\r",
									"                \"zip\": false,\r",
									"                \"ondemand\": false,\r",
									"                \"verb\": \"put\",\r",
									"                \"description\": \"Results\",\r",
									"                \"required\": false,\r",
									"                \"localName\": \"result.rvt\"\r",
									"              }\r",
									"            },\r",
									"            \"engine\": \"Autodesk.Revit+2026\",\r",
									"            \"appbundles\": [ `${pm.environment.get('dasNickName')}.${pm.environment.get(\"appBundleName\")}+test` ],\r",
									"            \"description\": \"Deletes walls from Revit file.\"\r",
									"          }));"
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
